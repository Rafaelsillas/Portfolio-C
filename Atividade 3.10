#include <stdio.h>;
// Rafael Sillas Valente da Costa

void maiorNumero(int num1, int num2);
int main() {
int a, b;
scanf("%d %d", &a, &b);
maiorNumero(a, b);
return 0;
}
void maiorNumero(int num1, int num2) {
if (num1 &gt; num2)
printf("%d é o maior\n";, num1);
else if (num1 &lt; num2)
printf("%d é o maior\n", num2);
else
printf("Os números são iguais\n");
}
