#include <stdio.h>
// Rafael Sillas Valente da Costa

int menorNumero(int num1, int num2, int num3);

int main() {
int n1, n2, n3;
scanf("%d %d %d", &n1, &n2, &n3);
int resultado = menorNumero(n1, n2, n3);
printf("O menor número é: %d\n", resultado);
return 0;
}
int menorNumero(int num1, int num2, int num3) {
int menor = num1;
if (num2 < menor) menor = num2;
if (num3 < menor) menor = num3;
return menor;
}
