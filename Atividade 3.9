#include <stdio.h>
// Rafael Sillas Valente da Costa

int multiplica(int vet[], int n) {
int mult = 1;
for (int i = 0; i < n; i++)
mult *= vet[i];
return mult;
}
int main() {
int n;
scanf("%d", &n);
int vet[n];
for (int i = 0; i < n; i++)
scanf("%d", &vet[i]);
int resultado = multiplica(vet, n);
printf("Multiplicação: %d\n", resultado);
return 0;
}
